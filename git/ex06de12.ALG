Algoritmo "semnome"

Var
velo : real
ms : real
rio : real
Inicio
escreval ("em quantos km/h voce dirigia?")
 leia (velo)
  ms <- velo / 3.6
   escreval (" voce dirige em ", ms,"ms/s")
    rio <- 434 / velo
     escreval ("nessa velocidade voce ira demorar ", rio ," horas até o Rio")
     


Fimalgoritmo